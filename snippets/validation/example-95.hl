// Create an endpoint taking name and age and concatenating these with a greeting
.arguments
   name:string
   age:int
.arguments
   name:string
   age:int
validators.mandatory:x:@.arguments/*/name
validators.mandatory:x:@.arguments/*/age
strings.concat
   .:"Hello "
   get-value:x:@.arguments/*/name
   .:" You are "
   get-value:x:@.arguments/*/age
   .:" years old"
yield
   message:x:@strings.concat