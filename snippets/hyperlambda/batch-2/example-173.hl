/*
 * Build a lambda object representing a table, inject dynamic content into it,
 * and convert it to HTML using [lambda2html].
 */
.table
   table
      tr
         th
            #text:Name
         th
            #text:Age
      tr
         td
            #text:Alice
         td
            #text:int:30
      tr
         td
            #text:Bob
         td
            #text:int:42

lambda2html:x:@.table/*
