/*
 * Connects to the "crm" SQL Server database and updates the "email"
 * of the user with ID 42 in the "users" table.
 */
mssql.connect:crm
   mssql.update
      table:users
      values
         email:new.email@example.com
      where
         and
            id.eq:int:42
yield
   message:"User email updated"
