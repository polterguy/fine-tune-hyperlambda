                                                unwrap:x:+/*/*
                                                sockets.signal:x:@.session
                                                   args
                                                      function_result:x:@config.get
                                                      invocation:x:@execute/*/json
                                                      file:x:@execute/*/workflow
                                                if
                                                   exists:x:@execute/*/result/*
