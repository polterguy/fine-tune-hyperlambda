                        .lambda
                           if
                              neq:x:@.no-invocation
                                 .:int:0
                              .lambda
                                 math.increment:x:@.no-invocation
                           if
                              and
                                 strings.contains:x:@.result
                                    .:___
                                 strings.contains:x:@.result
                                    .:FUNCTION_INVOCATION[
                              .lambda
                                 if
                                    eq:x:@.no-invocation
                                       .:int:0
                                    .lambda
