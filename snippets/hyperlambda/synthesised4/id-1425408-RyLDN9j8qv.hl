
// Checks if the [hyperlambda] argument exists and its value is not equal to either null or an empty string.
and
   exists:x:@.arguments/*/hyperlambda
   not
      eq
         get-value:x:@.arguments/*/hyperlambda
         .
   not
      eq
         get-value:x:@.arguments/*/hyperlambda
         .:
