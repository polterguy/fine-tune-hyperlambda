/*
 * Connect to the `university` database and join `students` with `enrollments`.
 * Retrieve 10 records starting from the 11th row â€” for paginated student enrollment data.
 */
data.connect:university
   data.read
      table:students
         join:enrollments
            type:inner
            on
               and
                  students.id:enrollments.student_id
      columns
         students.name
         enrollments.course_id
      limit:int:10
      offset:int:10
