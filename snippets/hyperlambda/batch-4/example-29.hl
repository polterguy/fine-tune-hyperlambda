/*
 * Posts a list of product objects with title and price fields to the inventory endpoint at 'https://inventory.example.com/api/products'.
 */
http.post:"https://inventory.example.com/api/products"
   convert:bool:true
   payload
      .
         title:Wireless Mouse
         price:decimal:29.99
      .
         title:Mechanical Keyboard
         price:decimal:79.95
      .
         title:HD Monitor
         price:decimal:149.50
