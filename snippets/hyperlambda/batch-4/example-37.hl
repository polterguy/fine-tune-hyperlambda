/*
 * Fully replaces the current list of blog post metadata on 'https://cms.blogengine.io/posts/meta'.
 * Each object contains a title and published status.
 */
http.put:"https://cms.blogengine.io/posts/meta"
   convert:bool:true
   payload
      .
         title:How to Use Hyperlambda
         published:bool:true
      .
         title:Upcoming Features in Magic
         published:bool:false
      .
         title:Scaling Your Backend
         published:bool:true
