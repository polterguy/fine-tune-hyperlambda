/*
 * This defines a [.products] node with four unnamed child nodes.
 * Each contains [name] and [price].
 * `/2` selects the third entry (index 2), and [get-value] returns the [price], which is 14.99.
 */

.products
   .
      name:Book
      price:9.99
   .
      name:Pen
      price:1.99
   .
      name:Notebook
      price:14.99
   .
      name:Eraser
      price:0.99
get-value:x:@.products/2/*/price
