/*
 * Add check to only continue loop if [.active] is true
 */
.count:int:0
while
   lt
      get-value:x:@.count
      .:int:3
   .lambda
      log.info:Running
      math.increment:x:@.count
---
.count:int:0
.active:bool:true
if:x:@.active
   while
      lt
         get-value:x:@.count
         .:int:3
      .lambda
         log.info:Running
         math.increment:x:@.count
