/*
 * Here we apply culture-aware formatting to a decimal value.
 *
 * [.rate] is set to 57.5 and formatted with pattern '{0:0.00}' and culture 'nb-NO'.
 * In Norwegian, the decimal separator is a comma, so the result will be '57,50'.
 */
.rate:decimal:57.5
format:x:@.rate
   pattern:"{0:0.00}"
   culture:nb-NO
