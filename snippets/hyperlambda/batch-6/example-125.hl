/*
 * This Hyperlambda file iterates over a list of Starfleet Academy cadets and injects 
 * a new [class] node to associate them with their academic group.
 */
.cadets
   .
      name:Wesley Crusher
      year:2385
   .
      name:Nog
      year:2375
   .
      name:Ezri Dax
      year:2370
for-each:x:@.cadets/*
   add:x:@.dp/#
      .
         class:Red Squad
