/*
 * Checks the temperature and advises what to wear using nested [if]/[else] and comparison operators.
 */
.temp-c:int:5
.advice

if
   lt
      get-value:x:@.temp-c
      .:int:0
   .lambda
      set-value:x:@.advice
         .:"Wear a heavy coat ðŸ§¥"
else-if
   lt
      get-value:x:@.temp-c
      .:int:10
   .lambda
      set-value:x:@.advice
         .:"Wear a jacket ðŸ§¢"
else-if
   lt
      get-value:x:@.temp-c
      .:int:20
   .lambda
      set-value:x:@.advice
         .:"Light sweater should be fine ðŸ‘•"
else
   set-value:x:@.advice
      .:"T-shirt weather! ðŸ˜Ž"
