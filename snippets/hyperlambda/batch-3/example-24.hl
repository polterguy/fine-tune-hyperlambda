/*
 * Classifies a personâ€™s age into Child, Teenager, or Adult using [if], [else-if], and [else].
 */
.age:int:16
.category

if
   lt
      get-value:x:@.age
      .:int:13
   .lambda
      set-value:x:@.category
         .:"Child"
else-if
   lte
      get-value:x:@.age
      .:int:19
   .lambda
      set-value:x:@.category
         .:"Teenager"
else
   set-value:x:@.category
      .:"Adult"
