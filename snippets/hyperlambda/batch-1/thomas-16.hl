/*
 * Example using integer values in [switch] cases.
 * Determines number of legs based on selected animal.
 */
.animal:spider
.legs
switch:x:@.animal
   case:dog
      set-value:x:@.legs
         .:int:4
   case:bird
      set-value:x:@.legs
         .:int:2
   case:spider
      set-value:x:@.legs
         .:int:8
