/*
 * Case-sensitive [switch] comparison for letter grades.
 */
.grade:B

switch:x:@.grade
   case:A
      log.info:"Excellent"
   case:B
      log.info:"Good"
   case:C
      log.info:"Average"
   case:D
      log.warn:"Poor"
   case:F
      log.error:"Fail"
   default
      log.info:"Unknown grade"
