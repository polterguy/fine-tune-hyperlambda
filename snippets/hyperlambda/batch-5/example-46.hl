/*
 * Returns the fourth score from a list of player scores.
 *
 * `[3,1]` is used to select just the fourth item (zero-based indexing).
 */
.arguments
.scores
   .
      player:Anna
      score:int:88
   .
      player:Ben
      score:int:92
   .
      player:Chris
      score:int:75
   .
      player:Diana
      score:int:89
yield
   scores:x:@.scores/*/[3,1]
