/*
 * Returns the first 3 product records from a static list.
 *
 * Uses sub-tree indexing `[0,3]` to select the first three items from the [.products] collection.
 */
.arguments
.products
   .
      name:Laptop
      price:1200
   .
      name:Monitor
      price:300
   .
      name:Keyboard
      price:75
   .
      name:Mouse
      price:40
yield
   products:x:@.products/*/[0,3]
