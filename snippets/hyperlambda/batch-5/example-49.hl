/*
 * Returns 2 city records starting from the third entry (index 2).
 *
 * This uses `[2,2]` to slice out a sub-array of city records.
 */
.arguments
.cities
   .
      name:New York
      country:USA
   .
      name:London
      country:UK
   .
      name:Berlin
      country:Germany
   .
      name:Tokyo
      country:Japan
yield
   cities:x:@.cities/*/[2,2]
