/*
 * Dynamically adds a new task to the [.tasks] node.
 *
 * Notice, it is crucial to use [unwrap] when adding nodes containing expressions.
 */
.tasks
   .
      title:Deploy
   .
      title:Test
   .
      title:Document
.title:QA
unwrap:x:+/*/*/*
add:x:@.tasks
   .
      .
         title:x:@.title
