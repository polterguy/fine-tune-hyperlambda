
// Set default HTTP headers for [.arguments]/*/headers using [validators.default], specifying "User-Agent" as "AINIRO-Crawler 2.0", "Accept-Encoding" as "identity", and "Accept" as "text/xml".
validators.default:x:@.arguments/*/headers
   User-Agent:AINIRO-Crawler 2.0
   Accept-Encoding:identity
   Accept:text/xml
